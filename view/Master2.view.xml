	<mvc:View xmlns:mvc="sap.ui.core.mvc" 
	xmlns:core="sap.ui.core" 
	xmlns="sap.m" 
	controllerName="au.com.bpse.view.Master2">
	<Page id="master2Page" 
	showNavButton="true" 
	navButtonPress="onNavBack" 
	title="{i18n&gt;master2Title}">
		<subHeader>
			<Bar id="master2SearchBar">
				<contentMiddle>
					<SearchField id="master2SearchField" 
					showRefreshButton="{device&gt;/isNoTouch}" 
					placeholder="{i18n&gt;master2SearchPlaceholder}"
					search="onSearch" 
					tooltip="{i18n&gt;master2SearchTooltip}" 
					width="100%">
					</SearchField>
				</contentMiddle>
			</Bar>
		</subHeader>
		<content>
			<List id="master2List" 
			items="{Operations}"
			mode="{device&gt;/listMode}" 
			noDataText="{i18n&gt;masterListNoDataText}" 
			select="onSelect" 
			growing="true" 
			growingScrollToLoad="true" 
			headerText="">
				<items>
					<ObjectListItem 
					id="master2ListItem" 
					type="{device&gt;/listItemType}" 
					press="onSelect" 
					title="Operation:" 
					number="{Inspoper}" 
					numberUnit="">
						<attributes>
							<ObjectAttribute text=""/>
						</attributes>
						<firstStatus>
							<ObjectStatus text=""/>
						</firstStatus>
					</ObjectListItem>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page></mvc:View>